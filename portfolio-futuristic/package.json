{
  "name": "harsh-pandey-futuristic-portfolio",
  "version": "2.0.0",
  "description": "Futuristic portfolio website for Harsh Pandey built with React and Tailwind CSS",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.534.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-simple-typewriter": "^5.0.1",
    "react-tsparticles": "^2.12.2",
    "three": "^0.155.0",
    "tsparticles": "^3.8.1",
    "vanta": "^0.5.24",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=build",
    "deploy:surge": "npm run build && surge build harsh-pandey-portfolio.surge.sh",
    "analyze": "npm run build && npx serve -s build",
    "predeploy": "npm run build",
    "deploy:gh": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "keywords": [
    "portfolio",
    "react",
    "tailwind",
    "futuristic",
    "glassmorphism",
    "frontend",
    "blockchain",
    "web3"
  ],
  "author": "Harsh Pandey",
  "license": "MIT"
}
